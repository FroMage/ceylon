package com.redhat.ceylon.compiler.test.structure.attribute;

abstract class ClassVariable {
    private ceylon.language.String publicvar;
    
    public final ceylon.language.String getPublicvar() {
        return this.publicvar;
    }
    
    public final void setPublicvar(ceylon.language.String publicvar) {
        this.publicvar = publicvar;
    }
    private ceylon.language.String capturedvar;
    
    private final ceylon.language.String getCapturedvar() {
        return this.capturedvar;
    }
    
    private final void setCapturedvar(ceylon.language.String capturedvar) {
        this.capturedvar = capturedvar;
    }
    
    public abstract ceylon.language.String getFormalvar();
    
    public abstract void setFormalvar(ceylon.language.String formalvar);
    
    private final void m() {
        setCapturedvar(.ceylon.language.String.instance("new value"));
    }
    
    ClassVariable() {
        ceylon.language.String uncapturedvar;
        setPublicvar(.ceylon.language.String.instance(""));
        uncapturedvar = .ceylon.language.String.instance("");
        setCapturedvar(.ceylon.language.String.instance(""));
        setFormalvar(.ceylon.language.String.instance(""));
    }
}