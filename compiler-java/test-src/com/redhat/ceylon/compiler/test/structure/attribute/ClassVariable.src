package com.redhat.ceylon.compiler.test.structure.attribute;

class ClassVariable {
    private String publicvar;
    private String capturedvar;
    
    private final void m() {
        setCapturedvar(.ceylon.language.String.instance("new value"));
    }
    
    ClassVariable() {
        String uncapturedvar;
        setPublicvar(.ceylon.language.String.instance(""));
        uncapturedvar = .ceylon.language.String.instance("");
        setCapturedvar(.ceylon.language.String.instance(""));
    }
    
    public final String getPublicvar() {
        return this.publicvar;
    }
    
    public final void setPublicvar(String publicvar) {
        this.publicvar = publicvar;
    }
    
    private final String getCapturedvar() {
        return this.capturedvar;
    }
    
    private final void setCapturedvar(String capturedvar) {
        this.capturedvar = capturedvar;
    }
}