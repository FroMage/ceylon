package com.redhat.ceylon.compiler.test.structure.attribute;

abstract class ClassAttribute {
    private final ceylon.language.String publicvar;
    
    public final ceylon.language.String getPublicvar() {
        return this.publicvar;
    }
    private final ceylon.language.String capturedvar;
    
    private final ceylon.language.String getCapturedvar() {
        return this.capturedvar;
    }
    
    public abstract ceylon.language.String getFormalvar();
    
    private final ceylon.language.String m() {
        return getCapturedvar();
    }
    
    ClassAttribute() {
        final ceylon.language.String uncapturedvar;
        publicvar = .ceylon.language.String.instance("");
        uncapturedvar = .ceylon.language.String.instance("");
        capturedvar = .ceylon.language.String.instance("");
    }
}