package com.redhat.ceylon.compiler.test.structure.attribute;

abstract class ClassAttribute {
    private final String publicvar;
    private final String capturedvar;
    
    private final String m() {
        return getCapturedvar();
    }
    
    ClassAttribute() {
        final String uncapturedvar;
        publicvar = .ceylon.language.String.instance("");
        uncapturedvar = .ceylon.language.String.instance("");
        capturedvar = .ceylon.language.String.instance("");
    }
    
    public final String getPublicvar() {
        return this.publicvar;
    }
    
    private final String getCapturedvar() {
        return this.capturedvar;
    }
    
    public abstract String getFormalvar();
}