package com.redhat.ceylon.compiler.test.expression.boxing;

class BooleanBoxing {
    
    private final void m() {
        final boolean b1 = true;
        final boolean b2 = !b1;
        final boolean b3 = negate(b2);
    }
    
    private final boolean negate(final boolean b) {
        return !b;
    }
    
    BooleanBoxing() {
    }
}