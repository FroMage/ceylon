class InitializerParamHidingGetter {
    private final ceylon.language.Boolean b;
    
    private final ceylon.language.Boolean getB() {
        return b;
    }
    
    InitializerParamHidingGetter(ceylon.language.Boolean b) {
        this.b = b;
        b.equals(ceylon.language.$true.getTrue());
    }
