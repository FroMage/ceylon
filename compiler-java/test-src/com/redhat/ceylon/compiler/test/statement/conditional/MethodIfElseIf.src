package com.redhat.ceylon.compiler.test.statement.conditional;

class MethodIfElseIf {
    
    private final ceylon.language.Natural m() {
        if (true == true) {
            return .ceylon.language.Natural.instance(1L);
        } else {
            if (false == true) {
                return .ceylon.language.Natural.instance(2L);
            } else {
                return .ceylon.language.Natural.instance(3L);
            }
        }
    }
    
    MethodIfElseIf() {
    }
}