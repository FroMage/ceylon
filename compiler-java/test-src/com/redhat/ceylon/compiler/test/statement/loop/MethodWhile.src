package com.redhat.ceylon.compiler.test.statement.loop;

class MethodWhile {
    
    public final long m() {
        boolean cont = true;
        while (cont) {
            cont = false;
        }
        return 0L;
    }
    
    MethodWhile() {
    }
    
    public static void main(.java.lang.String[] args) {
        ceylon.language.process.getProcess().setupArguments(args);
        new MethodWhile();
    }
}