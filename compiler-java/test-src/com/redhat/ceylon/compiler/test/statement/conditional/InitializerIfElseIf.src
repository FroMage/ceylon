package com.redhat.ceylon.compiler.test.statement.conditional;

class InitializerIfElseIf {
    
    InitializerIfElseIf() {
        boolean b;
        if (true) {
            b = true;
        } else {
            if (false) {
                b = false;
            } else {
                b = true;
            }
        }
    }
}