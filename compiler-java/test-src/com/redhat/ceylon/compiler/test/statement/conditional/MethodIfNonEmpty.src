package com.redhat.ceylon.compiler.test.statement.conditional;

class MethodIfNonEmpty extends .ceylon.language.Object {
    
    public final Natural m(final Sequence<Natural> x) {
        if (!x.isEmpty()) {
            return .ceylon.language.Natural.instance(1L);
        }
        return .ceylon.language.Natural.instance(0L);
    }
    
    MethodIfNonEmpty() {
    }
}